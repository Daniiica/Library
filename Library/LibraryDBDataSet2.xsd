FROM            Rentals AS r INNER JOIN
                         Books AS b ON b.BookID = r.BookID INNER JOIN
						 Authors AS a ON a.AuthorID = b.AuthorID INNER JOIN
                         Users AS u ON u.UserID = r.UserID</CommandText>
